package com.data;

public class ThrowCatchSingleMethod {
	public void throwException() {
		try {
			String arr[] = new String[5];
			arr[0]="Pramod";
			arr[1]="mahesh";
			System.out.println(arr[2].toUpperCase());
			
		}

		catch (Exception e) {
			e.printStackTrace();
		}
	}

	public static void main(String[] args) {
		new ThrowCatchSingleMethod().throwException();
	}

}
