package com.data;

class UserException {
	
	public UserException(String s) {
		System.out.println(s);
	}
	
}

public class TryCatchSingleMethod 
{
	public void throwException() {
		
		try {
			
			if(10 % 0 ==0) {
				throw new UserException("Math.ioException");
			}
			
		} catch (Exception e) {
			
		}
	}

}
