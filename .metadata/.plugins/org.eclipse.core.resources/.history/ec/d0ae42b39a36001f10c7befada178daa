package com.data;

public class SingleCatchBlock {
	public void meth1() {

		try {
		    int c= 10% 0;
			String arr[] = new String[5];
			arr[0] = "mahadev";
			arr[1] = "krishna";
			arr[2].toUpperCase();
			
		} catch (NullPointerException a) 
		{
			a.printStackTrace();
		}
		catch (ArithmeticException e) {
			e.printStackTrace();
			
		}
	}

	public static void main(String[] args) {
		new SingleCatchBlock().meth1();
	}

}
