package com.data;

public class UniqueNumber 
{
	public static void main(String[] args) {
		int arr[]= {3,7,7,1,3,3,8};
		int count=0;
		for(int i=1;i<=arr.length;i++) {
			int key=arr[i];
			for(int j=0;j<=arr.length-1;j++) {
				if(key==arr[1+j]) {
					count++;
				}
			}
		}
		System.out.println(count);
		
	}
	

}
