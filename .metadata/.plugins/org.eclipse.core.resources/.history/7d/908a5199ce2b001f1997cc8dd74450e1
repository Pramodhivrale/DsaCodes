package in.codes;

import java.util.Scanner;

public class String 
{
	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		int lineNumber = 1;
		
		while(sc.hasNextLine()) {
	        java.lang.String line=sc.nextLine();
	        System.out.println(lineNumber +" "+line);
	        lineNumber++;
	        
			
		}
		
	}

}
