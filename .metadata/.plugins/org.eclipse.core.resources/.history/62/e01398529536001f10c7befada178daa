package com.data;

public class ThrowCatchSingleMethod 
{
	public void throwException() 
	{
		try {
			String name="pramod";
			System.out.println(name.indexOf("a"));
		}
		catch (Exception e) {
			// TODO: handle exception
		}
	}
	public static void main(String[] args) {
		new ThrowCatchSingleMethod().throwException();
	}

}
