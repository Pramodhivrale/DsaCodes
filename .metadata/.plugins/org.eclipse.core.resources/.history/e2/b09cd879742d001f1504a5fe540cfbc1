package in.codes;

import java.util.Scanner;

public class MathPaw {
	
	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		
		int n=sc.nextInt();
		
		int g= (int) Math.pow(a, n/2);
		
		if(n % 2==0) {
			int power=g * g;
			System.out.println(power);
		}
		
	}

}
