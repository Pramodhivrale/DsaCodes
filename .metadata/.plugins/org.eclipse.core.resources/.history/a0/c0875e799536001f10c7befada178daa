package com.data;

public class ThrowCatchSingleMethod {
	public void throwException() {
		try {
			String name[] = { "Pramod", "Rutika" };
			System.out.println();

		}

		catch (Exception e) {
            e.printStackTrace();
		}
	}

	public static void main(String[] args) {
		new ThrowCatchSingleMethod().throwException();
	}

}
